{
  "name": "Pi-hole-dev",
  "version": "2025.02.7",
  "slug": "pihole-dev",
  "description": "Network-wide Ad Blocking",
  "url": "https://github.com/casperklein/homeassistant-addons-dev/tree/master/pi-hole",
  "image": "casperklein/homeassistant-pihole-dev",
  "arch": ["aarch64", "amd64", "armhf", "armv7"],
  "webui": "[PROTO:ssl]://[HOST]:[PORT:80]",
  "ingress": true,
  "ingress_port": 80,
  "ingress_entry": "/",
  "panel_icon": "mdi:pi-hole",
  "startup": "system",
  "ports": {
    "53/tcp": 53,
    "53/udp": 53,
    "80/tcp": null,
    "443/tcp": null,
    "123/udp": null
  },
  "ports_description": {
    "53/tcp": "DNS server port",
    "53/udp": "DNS server port",
    "80/tcp": "UNPROTECTED web UI (not required for Ingress)",
    "443/tcp": "UNPROTECTED web UI (not required for Ingress)",
    "123/udp": "NTP server port (not required, unless NTP is activated in Pi-hole)"
  },
  "privileged": ["SYS_NICE"],
  "boot": "auto",
  "timeout": 60,
  "options": {
	  "dnscrypt": []
  },
  "schema": {
	  "dnscrypt": [
		  {
			  "name": "str",
			  "stamp": "str"
		  }
	  ]
  },
  "init": false
}
